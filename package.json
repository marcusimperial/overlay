{
  "name": "overlay",
  "version": "0.1.0",
  "main": "index.js",
  "author": "dare",
  "description": "overlay",
  "license": "ISC",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.dare.overlay",
    "productName":"overlay",
    "publish": [{
      "provider": "generic",
      "url": "https://storage.googleapis.com/overlayy/"
    }],
    "win": {
      "target": ["nsis"]
    },
    "nsis": {
      "artifactName":"overlaysetup.exe",
      "oneClick": true,
      "allowToChangeInstallationDirectory": false
    }
  },
  "dependencies": {
    "electron-updater": "^4.3.9",
    "mongodb": "^4.0.1",
    "node-fetch": "^2.6.1",
    "read-last-lines": "^1.8.0"
  },
  "devDependencies": {
    "electron": "^13.1.4",
    "electron-builder": "^22.11.7"
  }
}
