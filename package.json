{
  "name": "overlay",
  "version": "1.0.0",
  "description": "overlay",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcusimperial/overlay.git"
  },
  "scripts": {
    "start": "electron .",
    "build-installer": "electron-builder",
    "deploy": "electron-builder build --win --publish always"
  },
  "build": {
    "appId": "overlay",
    "publish": [{
      "provider": "github",
      "owner": "marcusimperial",
      "repo": "overlay",
      "token":"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"
    }],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/icons/win/icon.ico"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "assets/icons/win/icon.ico"
    },
    "dmg": {
      "icon": "assets/icons/win/icon.ico"
    },
    "nsis": {
      "installerIcon": "assets/icons/win/icon.ico",
      "uninstallerIcon": "assets/icons/win/icon.ico",
      "uninstallDisplayName": "CPU Monitor",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "author": "dare",
  "license": "ISC",
  "dependencies": {
    "electron-updater": "^4.3.9",
    "mongodb": "^4.0.1",
    "node-fetch": "^2.6.1",
    "read-last-lines": "^1.8.0"
  },
  "devDependencies": {
    "electron": "^13.1.4",
    "electron-builder": "^22.11.7"
  }
}
